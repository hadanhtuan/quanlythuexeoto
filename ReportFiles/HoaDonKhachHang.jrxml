	<subDataset name="Table Dataset 1" uuid="38b57213-6349-46cf-8a43-901ed4303080">
		<parameter name="MADX" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="MaND" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select n.TenND, d.MaDX ,d.NgayNhanXe, d.NgayTraXe, c.MaLX, l.TenLX, c.SoLuong, l.Gia, (d.NgayTraXe - d.NgayNhanXe) as SoNgay, ((c.SoLuong*l.Gia)*(d.NgayTraXe - d.NgayNhanXe)) as ThanhTien
from 
ChiTietDatXe c  join DonDatXe d
on c.MaDX =d.MaDX 
join LoaiXe l on
l.MaLX = c.MaLX
join NguoiDung n
on n.MaND = d.MaKH
where n.MaND = $P{MaND} and d.MaDX = $P{MADX}]]>
		</queryString>
		<field name="TENND" class="java.lang.String"/>
		<field name="MADX" class="java.lang.String"/>
		<field name="NGAYNHANXE" class="java.sql.Timestamp"/>
		<field name="NGAYTRAXE" class="java.sql.Timestamp"/>
		<field name="MALX" class="java.lang.String"/>
		<field name="TENLX" class="java.lang.String"/>
		<field name="SOLUONG" class="java.math.BigDecimal"/>
		<field name="GIA" class="java.lang.Double"/>
		<field name="SONGAY" class="java.math.BigDecimal"/>
		<field name="THANHTIEN" class="java.math.BigDecimal"/>
	</subDataset>
